<h2>Upload your pantograph here.</h2>

<%= form_for @personality_profile, url: { action: 'create' } do |f| %>
    <div class="form-group">
      <%= f.file_field(:pantograph, required: 'required') %>
    </div>
    <div class="form-group">
      <%= f.submit 'Upload', class: 'btn-primary' %>
    </div>
<% end %>
